#include<stdio.h>
#include<conio.h>
void main()
{
void swap(int*,int*);
int a,b;
clrscr();
printf("Enter a,b values:");
scanf("%d %d",&a,&b);
swap(&a,&b);
printf("after swapping a,b values are:\n a=%d\tb=%d",a,b);
